{
    "name": "sarfrazrizwan/laravel-merge-tags",
    "description": "A Laravel package for seamlessly merging model data into HTML templates.",
    "keywords": [
        "sarfrazrizwan",
        "laravel-merge-tags"
    ],
    "homepage": "https://github.com/sarfrazrizwan/laravel-merge-tags",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Rizwan Sarfraz",
            "email": "rs.mahaar@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Sarfrazrizwan\\LaravelMergeTags\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sarfrazrizwan\\LaravelMergeTags\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sarfrazrizwan\\LaravelMergeTags\\LaravelMergeTagsServiceProvider"
            ]
        }
    }
}
